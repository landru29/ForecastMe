{
	"process": {
		"nb-forks": "2",
		"binding-port": "3000"
	},
	"db": {
		"host": "localhost",
		"port": "27017",
		"database": "forecastmenow"
	},
	"log": {
		"file": "access.log"
	},
	"cors": [
		"http://localhost:9000"
	],
	"public": {
		"folder": "../client/dist",
		"default": "index.html"
	},
	"session": {
		"timeout": "10"
	},
	"acl": {
		"guest": [],
		"user": ["guest", "connected"],
		"admin": ["guest", "connected", "user"]
	},
	"resources": {
		"team": {
			"controller": "resources/team",
			"acl": {
				"GET": "guest"
			}
		},
		"team-collection": {
			"controller": "resources/team-collection",
			"acl": {
				"GET": "guest"
			}
		},
		"pool": {
			"controller": "resources/pool",
			"acl": {
				"GET": "guest"
			}
		},
		"pool-collection": {
			"controller": "resources/pool-collection",
			"acl": {
				"GET": "guest"
			}
		}
	}
}